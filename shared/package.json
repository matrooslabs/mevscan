{
  "name": "@mevscan/shared",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions and utilities for MEVScan",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./clickhouse": {
      "types": "./dist/clickhouse.d.ts",
      "default": "./dist/clickhouse.js"
    },
    "./ably": {
      "types": "./dist/ably.d.ts",
      "default": "./dist/ably.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "build:check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.7"
  },
  "devDependencies": {
    "typescript": "^5.6.0"
  }
}
