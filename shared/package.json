{
  "name": "@mevscan/shared",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions for MEVScan",
  "main": "dist/types.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js",
      "default": "./types.ts"
    },
    "./clickhouse": {
      "types": "./dist/clickhouse.d.ts",
      "import": "./dist/clickhouse.js",
      "require": "./dist/clickhouse.js",
      "default": "./clickhouse.ts"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js",
      "require": "./dist/config.js",
      "default": "./config.ts"
    },
    "./ably": {
      "types": "./dist/ably.d.ts",
      "import": "./dist/ably.js",
      "require": "./dist/ably.js",
      "default": "./ably.ts"
    },
    "./ablyConstants": {
      "types": "./dist/ablyConstants.d.ts",
      "import": "./dist/ablyConstants.js",
      "require": "./dist/ablyConstants.js",
      "default": "./ablyConstants.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.7",
    "ably": "^2.15.0",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "typescript": "^5.6.0"
  }
}
